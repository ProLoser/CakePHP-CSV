{
    "name": "proloser/cakephp-csv",
    "type": "cakephp-plugin",
    "require": {
        "composer/installers": "~1.0",
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakePHPCSV\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePHPCSV\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
